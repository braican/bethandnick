{
    "name": "braican/bethandnick",
    "description": "A WordPress backend for Beth and Nick's wedding",
    "type": "wordpress-theme",
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "advanced-custom-fields/advanced-custom-fields-pro",
                "version": "5.7.10",
                "type": "wordpress-plugin",
                "dist": {
                    "type": "zip",
                    "url": "https://connect.advancedcustomfields.com/index.php?p=pro&a=download&k=b3JkZXJfaWQ9ODExODZ8dHlwZT1wZXJzb25hbHxkYXRlPTIwMTYtMDUtMDkgMTI6NTM6Mzk=&t=5.7.10"
                }
            }
        },
        {
            "type": "vcs",
            "url": "https://github.com/braican/wp-netlify-deployer"
        }
    ],
    "require": {
        "advanced-custom-fields/advanced-custom-fields-pro": "5.7.10",
        "wpackagist-plugin/wp-migrate-db": "^1.0.9",
        "braican/netlify-deployer": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "BethAndNick\\": "lib/"
        }
    },
    "extra": {
        "installer-paths": {
            "../../plugins/{$name}/": [
                "type:wordpress-plugin"
            ]
        }
    }
}